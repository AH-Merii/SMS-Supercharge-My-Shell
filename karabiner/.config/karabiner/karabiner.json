      "complex_modifications": {
        "rules": [
          {
            "description": "Track Tab Switcher State",
            "manipulators": [
              {
                "type": "basic",
                "from": {
                  "key_code": "tab",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["shift"]
                  }
                },
                "to": [
                  { "key_code": "tab", "modifiers": ["left_control"] },
                  { "set_variable": { "name": "ctrl_tab_active", "value": 1 } }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "tab",
                  "modifiers": {
                    "mandatory": ["option"],
                    "optional": ["shift"]
                  }
                },
                "to": [
                  { "key_code": "tab", "modifiers": ["left_option"] },
                  { "set_variable": { "name": "alt_tab_active", "value": 1 } }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "left_control"
                },
                "to": [{ "key_code": "left_control" }],
                "to_after_key_up": [
                  { "set_variable": { "name": "ctrl_tab_active", "value": 0 } }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "right_control"
                },
                "to": [{ "key_code": "right_control" }],
                "to_after_key_up": [
                  { "set_variable": { "name": "ctrl_tab_active", "value": 0 } }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "left_option"
                },
                "to": [{ "key_code": "left_option" }],
                "to_after_key_up": [
                  { "set_variable": { "name": "alt_tab_active", "value": 0 } }
                ]
              },
              {
                "type": "basic",
                "from": {
                  "key_code": "right_option"
                },
                "to": [{ "key_code": "right_option" }],
                "to_after_key_up": [
                  { "set_variable": { "name": "alt_tab_active", "value": 0 } }
                ]
              }
            ]
          },
